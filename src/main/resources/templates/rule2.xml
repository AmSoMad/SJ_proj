              </PolygonSymbolizer>
              <LineSymbolizer>
                  <Stroke>
                  <CssParameter name="stroke">
                      <ogc:Literal>#00c8ff</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="stroke-width">
                      <ogc:Literal>2</ogc:Literal>
                  </CssParameter>
                  </Stroke>
                  <Fill>
                  <CssParameter name="Fill">
                      <ogc:Literal>#00c8ff</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="Fill-width">
                      <ogc:Literal>0.5</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="fill-opacity">
                      <ogc:Literal>0.5</ogc:Literal>
                      </CssParameter>                
                  </Fill>            
              </LineSymbolizer>
              <TextSymbolizer>
                  <Label>
                      <ogc:PropertyName>A9</ogc:PropertyName>
                  </Label>
                  <Font>
                  <CssParameter name="font-family">휴먼편지체</CssParameter>               
                  <CssParameter name="font-size">13</CssParameter>
                  <CssParameter name="font-style">normal</CssParameter>
                  <CssParameter name="font-weight">normal</CssParameter>
                  </Font>
                  <LabelPlacement>
                  <PointPlacement>
                      <AnchorPoint>
                      <AnchorPointX>0.5</AnchorPointX>
                      <AnchorPointY>0.5</AnchorPointY>
                      </AnchorPoint>
                  </PointPlacement>
                  </LabelPlacement>
                  <Halo>
                  <Radius>3</Radius>
                  <Fill>
                      <CssParameter name="fill">#00c8ff</CssParameter>
                  </Fill>
                  </Halo>         
                  <VendorOption name="autoWrap">60</VendorOption>
                  <VendorOption name="maxDisplacement">90</VendorOption>
              </TextSymbolizer>
        </Rule> 
		<Rule><!--1종근린-->
            <Title>etc_03000</Title>
            <ogc:Filter>
              <ogc:PropertyIsEqualTo>
                <ogc:PropertyName>A8</ogc:PropertyName>
                <ogc:Literal>03000</ogc:Literal>
                <ogc:Literal>Z3000</ogc:Literal>
              </ogc:PropertyIsEqualTo>
            </ogc:Filter>
              <PolygonSymbolizer>
                  <Stroke>
                  <CssParameter name="stroke">#000000</CssParameter>
                  <CssParameter name="stroke-width">0.5</CssParameter>
                  </Stroke>
              </PolygonSymbolizer>
              <LineSymbolizer>
                  <Stroke>
                  <CssParameter name="stroke">
                      <ogc:Literal>#0064ff</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="stroke-width">
                      <ogc:Literal>2</ogc:Literal>
                  </CssParameter>
                  </Stroke>
                  <Fill>
                  <CssParameter name="Fill">
                      <ogc:Literal>#0064ff</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="Fill-width">
                      <ogc:Literal>0.5</ogc:Literal>
                  </CssParameter>
                  <CssParameter name="fill-opacity">
                      <ogc:Literal>0.5</ogc:Literal>
                      </CssParameter>                
                  </Fill>            
              </LineSymbolizer>
              <TextSymbolizer>
                  <Label>
                      <ogc:PropertyName>A9</ogc:PropertyName>
                  </Label>
                  <Font>
                  <CssParameter name="font-family">휴먼편지체</CssParameter>               
                  <CssParameter name="font-size">13</CssParameter>
                  <CssParameter name="font-style">normal</CssParameter>
                  <CssParameter name="font-weight">normal</CssParameter>
                  </Font>
                  <LabelPlacement>
                  <PointPlacement>
                      <AnchorPoint>
                      <AnchorPointX>0.5</AnchorPointX>
                      <AnchorPointY>0.5</AnchorPointY>
                      </AnchorPoint>
                  </PointPlacement>
                  </LabelPlacement>
                  <Halo>
                  <Radius>3</Radius>
                  <Fill>
                      <CssParameter name="fill">#0064ff</CssParameter>
                  </Fill>
                  </Halo>         
                  <VendorOption name="autoWrap">60</VendorOption>
                  <VendorOption name="maxDisplacement">90</VendorOption>
              </TextSymbolizer>
        </Rule>                         
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>
